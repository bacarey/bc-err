{
  "source": {
    "include": [
      ".",
      "README.md",
      "package.json"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "tags": {
    "allowUnknownTags": [],
    "dictionaries": [
      "jsdoc"
    ]
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs/",
    "recurse": true
  }
}
